<?xml version="1.0"?>
<parameters>
    <!-- Connection details -->
    <dbtype>postgres</dbtype>
    <driver>org.postgresql.Driver</driver>
    <DBUrl>jdbc:postgresql://127.0.0.1:5432/web3bench</DBUrl>
    <username>web3bench</username>
    <password>web3bench</password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    <distribution>rand</distribution>
    <uploadUrl></uploadUrl>

    <nodeid>main</nodeid>
    <scalefactor>3</scalefactor>

    <!-- The workload -->
    <terminals>1</terminals>
    <works>
        <work>
            <serial>true</serial>
            <rate>unlimited</rate>
            <weights>1,1,1,1,1,1,1,1,1</weights>
        </work>
    </works>

    <transactiontypes>
        <transactiontype>
            <name>R31</name>
        </transactiontype>
        <transactiontype>
            <name>R32</name>
        </transactiontype>
        <transactiontype>
            <name>R33</name>
        </transactiontype>
        <transactiontype>
            <name>R34</name>
        </transactiontype>
        <transactiontype>
            <name>R35</name>
        </transactiontype>
        <transactiontype>
            <name>W2</name>
        </transactiontype>
        <transactiontype>
            <name>W3</name>
        </transactiontype>
        <transactiontype>
            <name>W51</name>
        </transactiontype>
        <transactiontype>
            <name>W52</name>
        </transactiontype>
    </transactiontypes>
</parameters>
