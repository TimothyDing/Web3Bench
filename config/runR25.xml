<?xml version="1.0"?>
<parameters>
    <!-- Connection details -->
    <dbtype>postgres</dbtype>
    <driver>org.postgresql.Driver</driver>
    <DBUrl>jdbc:postgresql://127.0.0.1:5432/web3bench</DBUrl>
    <username>web3bench</username>
    <password>web3bench</password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    <distribution>rand</distribution>
    <uploadUrl></uploadUrl>

    <nodeid>main</nodeid>
    <scalefactor>3</scalefactor>

    <!-- The workload -->
    <terminals>2</terminals>
    <works>
        <work>
            <warmup>0</warmup>
            <time>5</time>
            <rate>16</rate>
            <weights>100</weights>
            <arrival>REGULAR</arrival>
        </work>
    </works>

    <transactiontypes>
        <transactiontype>
            <name>R25</name>
        </transactiontype>
    </transactiontypes>
</parameters>
